<?xml version="1.0" encoding="utf-8" ?>
<go-plugin id="@plugin.id@" version="1"
    xmlns="https://raw.githubusercontent.com/gocd/gocd/master/plugin-infra/go-plugin-api/src/main/resources/plugin-descriptor.xsd">
    <about>
        <name>@project.name@</name>
        <version>@project.version@</version>
        <target-go-version>@go.version@</target-go-version>
        <description>@project.description@</description>
        <vendor>
            <name>@author@</name>
            <url>@repo.url@</url>
        </vendor>
    </about>
</go-plugin>
